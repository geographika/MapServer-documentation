msgid "use wms_group_style_title to override the style title (optional)"
msgstr ""

# 4e198d200d0c4389849b430862437a70
#: ../../../en/development/rfc/ms-rfc-75.txt:296
msgid ""
"use wms_group_style_legendurl_* to override width, heigth, format and href "
