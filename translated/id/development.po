msgid "http://trac.osgeo.org/mapserver/ticket/2143"
msgstr ""

# b52d078683c248768fa53bf4becf2f3c
#: ../../../en/development/rfc/ms-rfc-31.txt:199
msgid "+1 from SteveL, SteveW, TomK, FrankW, AssefaY, PericlesN"
msgstr ""

# 0a8e825958b740f08021aefc72f37c66
#: ../../../en/development/rfc/ms-rfc-31.txt:201
